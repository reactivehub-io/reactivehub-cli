{
  "name": "the-cli",
  "version": "1.0.0",
  "description": "",
  "preferGlobal": true,
  "bin": {
    "rhub": "./dist/index.js"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist --source-maps && npm link",
    "lint": "eslint src/* --fix",
    "dev": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "aev": "NODE_ENV=development nodemon src/index.js aev --exec babel-node",
    "afl": "NODE_ENV=development nodemon src/index.js afl --exec babel-node",
    "adac": "NODE_ENV=development nodemon src/index.js add:action testing.new-event on-event GCP FIRESTORE --exec babel-node",
    "login": "NODE_ENV=development nodemon src/index.js login --exec babel-node",
    "deploy": "NODE_ENV=development nodemon src/index.js deploy --exec babel-node",
    "hlp": "nodemon src/index.js --help --exec babel-node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "configstore": "^4.0.0",
    "flat": "^4.1.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "opn": "^5.4.0",
    "yaml-js": "^0.2.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.13.0",
    "nodemon": "^1.18.3"
  }
}
